{
  "manifest_version": 2,
  "name": "RegExp Finder Devtool",
  "version": "1.0.0",
  "description": "RegExp Finder Devtool",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}